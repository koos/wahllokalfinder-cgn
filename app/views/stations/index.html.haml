#map{data: { wheelchair_stations: @with_wheelchair_stations.map{|s| {number: s.vote_district_id, street: s.address, name: s.name, geo: [s.longitude, s.latitude]}}.to_json, without_wheelchair_stations: @without_wheelchair_stations.map{|s| {number: s.vote_district_id, street: s.address, name: s.name, geo: [s.longitude, s.latitude]}}.to_json}}

.legend
  %h1 Wahllokalfinder
  .icon-list
    - if @without_wheelchair_stations.any?
      .icon-list--item
        = image_tag('marker1.png')
        = "Wahllokal (#{@without_wheelchair_stations.count})"
    - if @with_wheelchair_stations.any?
      .icon-list--item
        = image_tag('marker2.png')
        = "Rollstuhlgerechtes Wahllokal (#{@with_wheelchair_stations.count})"
