.headline
  %h2#cities
    WAHLLOKALFINDER

#boxes-wrapper
  .featured-box

#footer
  = render '/layouts/footer'
#map{data: { wheelchair_stations:{} ,without_wheelchair_stations:{}  } }
%script
  var geojson = {
  type: 'FeatureCollection',
  features: [
  - @cities.each do |city|
    = "{ type: 'Feature', geometry: {type: 'Point', coordinates: [#{city.coordinates}]},properties: {title: '#{city.name}', slug: '#{city.slug}', description: '#{city.name} - #{city.state}'}},".html_safe
  ]};
